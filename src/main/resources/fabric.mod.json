{
  "schemaVersion": 1,
  "id": "amrpcf",
  "version": "${version}",

  "name": "Amitoj's Minecraft RPC (Fabric)",
  "description": "A Discord RPC mod for Minecraft (Fabric)",
  "authors": [
    "Amitoj Singh"
  ],
  "contact": {
    "homepage": "https://github.com/amitojsingh366/Amitojs-Minecraft-RPC-Fabric",
    "sources": "https://github.com/amitojsingh366/Amitojs-Minecraft-RPC-Fabric"
  },

  "license": "MIT",
  "icon": "assets/amrpcf/logo.png",

  "environment": "*",
  "entrypoints": {
